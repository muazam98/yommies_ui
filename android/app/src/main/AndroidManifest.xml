<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.alNajah.yommie">
  <uses-permission android:name="android.permission.CAMERA" />
  <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
  <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
  <application android:name="io.flutter.app.FlutterApplication" android:label="Yomie's Rice x Yogurt Kuala Lumpur &amp; Selangor" android:requestLegacyExternalStorage="true" android:icon="@mipmap/ic_launcher">
